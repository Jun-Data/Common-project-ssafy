pipeline {
    agent any

    stages {
        stage('Gitlab Clone') {
            steps {
                git(
                    branch: 'master',
                    credentialsId: 'aseongjun99',
                    url: 'https://lab.ssafy.com/s12-webmobile1-sub1/S12P11A506.git'
                )
            }
        }

        stage('build') {
            steps {
                dir('Backend') {
                    sh 'chmod +x gradlew && ./gradlew clean bootJar'
                }
            }
        }

    }
}
