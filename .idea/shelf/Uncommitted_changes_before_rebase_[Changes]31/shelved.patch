<+>/* Grid → Flex 변환 핵심 부분 */\n.kioskmainscreen {\n    display: flex;\n    flex-direction: column;\n    max-width: 800px;\n    width: 100vw;\n    height: 100dvh;\n    background-color: #e9ecef;\n    overflow: hidden;\n}\n\n/* 공통 Flex 아이템 속성 */\n.kioskmainscreen > * {\n    width: 95%;\n    margin: 0 auto;\n}\n\n.mainscreencartpage {\n    flex: 1 1 38vh; /* 초기 높이 38vh 유지 */\n    min-height: 300px; /* 최소 높이 제한 */\n    box-shadow: 0 4px 6px rgba(0,0,0,0.1);\n    margin-top: 2vh;\n    overflow-y: auto; /* 스크롤 가능 영역 */\n}\n\n.kioskpleaproductlist {\n    flex: 1 1 38vh;\n    min-height: 300px;\n    box-shadow: 0 4px 6px rgba(0,0,0,0.1);\n    margin: 2vh auto;\n    overflow: hidden;\n}\n\n.barcodebuttoncontainer {\n    flex: 0 0 20vh; /* 고정 높이 영역 */\n    display: flex;\n    gap: 5%;\n    margin: 2vh auto;\n}\n\n/* 바코드 스캐너 섹션 */\n.mainscreenbarcodescanner {\n    flex: 1 1 40%;\n    min-width: 300px;\n}\n\n/* 버튼 영역 개선 */\n.buttonzone {\n    flex: 1 1 50%;\n    display: flex;\n    flex-direction: column;\n    gap: 15px;\n    justify-content: space-between;\n}\n\n.cartpagetotal {\n    font-size: clamp(1.5rem, 4vw, 2.5rem); /* 반응형 폰트 */\n    height: auto;\n    padding: 10px 0;\n}\n\n.mainscreenpayment,\n.mainscreentohome {\n    flex: 1;\n    min-height: 60px;\n    font-size: clamp(1.2rem, 3vw, 2rem);\n}\n\n.popup-overlay {\n    background-color: rgba(0, 0, 0, 0.7);\n}\n\n/* 쿠폰 팝업 조정 */\n.coupon-prompt-content {\n    width: min(600px, 90vw);\n    height: min(400px, 50vh);\n    font-size: clamp(1.5rem, 4vw, 2.2rem);\n}\n\n.coupon-prompt-buttons button {\n    width: min(160px, 30vw);\n    height: min(70px, 10vh);\n    font-size: clamp(1rem, 3vw, 1.5rem);\n}\n\n/* 가로 모드 대응 */\n@media (orientation: landscape) {\n    .kioskpleaproductlist {\n        flex-basis: 45vh;\n    }\n\n    .mainscreenpayment,\n    .mainscreentohome {\n        min-height: 80px;\n    }\n}\n
